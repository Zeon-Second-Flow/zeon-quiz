    "formik": "^2.2.9",
    "framer-motion": "^7.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "6.4.1",
    "react-select": "^5.4.0",
    "socket.io-client": "^4.5.2",
    "typescript": "^4.8.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "18.8.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
